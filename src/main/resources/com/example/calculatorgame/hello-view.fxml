<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="256.0" prefWidth="256.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.calculatorgame.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <AnchorPane prefHeight="200.0" prefWidth="200.0">
      <children>
      
          <Label alignment="CENTER" layoutY="26.0" prefHeight="44.0" prefWidth="220.0" text="Calculator Game" textAlignment="CENTER">
            <opaqueInsets>
               <Insets />
            </opaqueInsets>
            <font>
               <Font name="Power Clear" size="21.0" />
            </font>
         </Label>
          <Button alignment="CENTER" contentDisplay="CENTER" layoutX="35.0" layoutY="108.0" onAction="#startGame" text="Start" textAlignment="CENTER">
            <font>
               <Font name="Power Clear" size="16.0" />
            </font>
         </Button>
         <Button alignment="CENTER" contentDisplay="CENTER" layoutX="130.0" layoutY="108.0" onAction="#openHelp" text="Help" textAlignment="CENTER">
            <font>
               <Font name="Power Clear" size="16.0" />
            </font>
         </Button>
      </children>
   </AnchorPane>
</VBox>
